    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "@ionic/angular": "5.3.4",
    "rxjs": "~6.5.1",
    "sw-toolbox": "3.6.0",
    "zone.js": "~0.10.2",
    "typescript": "~3.8.3",
    "copyfiles": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arielfaur/ionic-audio.git"
