{
  "name": "ionic-audio",
  "version": "3.2.2",
  "description": "An audio player for Ionic 3 and Angular 5",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "node_modules/.bin/ngc && copyfiles -u 1 ./src/*.scss ./dist",
